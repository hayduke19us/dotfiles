tmux -2 new-session -s horizon -n editor -d
tmux -2 send-keys -t horizon 'cd ~/code/horizon' C-m
tmux -2 send-keys -t horizon 'vim' C-m
tmux -2 split-window -v -t horizon
tmux -2 split-window -v -t horizon
tmux -2 select-layout -t horizon main-vertical
tmux -2 send-keys -t horizon:1.2 'cd ~/code/horizon' C-m
tmux -2 send-keys -t horizon:1.2 './run_tests.sh --runserver' C-m
tmux -2 new-window -n forward -t horizon
tmux -2 send-keys -t horizon:2 'cd ~' C-m
tmux -2 send-keys -t horizon:2 'forward 8000' C-m
tmux -2 select-window -t horizon:1
tmux -2 attach -t horizon
